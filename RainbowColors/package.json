{
  "name": "RainbowColors",
  "displayName": "RainbowColors",
  "description": "This extension todo",
  "version": "1.0.1",
  "publisher": "dennykorsukewitz",
  "icon": "doc/images/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/dennykorsukewitz/VSCode-RainbowColors/blob/dev/README.md",
  "pricing": "Free",
  "bugs": {
    "url": "https://github.com/dennykorsukewitz/VSCode-RainbowColors/issues",
    "email": "dennykorsukewitz@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dennykorsukewitz/VSCode-RainbowColors"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/dennykorsukewitz"
  },
  "keywords": [
    "todo"
  ],
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "lint-fix": "eslint src --ext ts  --fix",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  },
  "contributes": {
    "commands": [
      {
        "command": "RainbowColors.helloWorld",
        "title": "Hello World"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "GitHub File Fetcher",
      "properties": {
        "RainbowColors.todo": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "description": "List of possible GitHub repositories. GitHub {owner}/{repo}.",
          "default": [
            "dennykorsukewitz/VSCode-RainbowColors"
          ]
        }
      }
    }
  }
}